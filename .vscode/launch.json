{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug run go",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/main.go",
      "cwd": "${workspaceFolder}",
      "env": {
        //       "MICRO_STARTER_CONFIG_PATH": "./",
        "MICRO_STARTER_DEBUG_MODE": "true",
        "DBType": "dgraph",
        "RedisHost": "redis://127.0.0.1:6379",
        "PubTopics": "alarm,status,event",
      },
      "args": [
        "--profile=dev",
        "--service_address=127.0.0.0.1:8080",
        "--c=config.json"
      ]
    }
  ]
}
